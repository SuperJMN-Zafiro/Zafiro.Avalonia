<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:AvaloniaApplication1"
             xmlns:libVlcSharp="clr-namespace:Zafiro.Avalonia.LibVLCSharp;assembly=Zafiro.Avalonia.LibVLCSharp"
             x:Class="AvaloniaApplication1.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Resources>
        <libVlcSharp:StringToVideoSourceConverter x:Key="StringToVideoSourceConverter" />
    </Application.Resources>

    <Application.Styles>
        <FluentTheme Mode="Light"/>
    </Application.Styles>
</Application>
